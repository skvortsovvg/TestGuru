<h1>Новый вопрос к тесту:</h1>

<form action="<%= test_questions_path(@test) %>" method="post">
  <input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
  <p>
    <label for="qst-body">Вопрос:</label>
    <input type="text" name="question[body]" id="qst-body">
  </p>
  
  <input type="submit" value="Добавить">
</form>
